[project]
	description = The Qlalr repository\n\
\n\
ssh://codereview.qt-project.org:29418/qt/qlalr.git
[receive]
	requireChangeId = true
	requireContributorAgreement = true
[submit]
	action = cherry pick
	mergeContent = true
[cherryPick]
	includeReviewedOn = true
[cherryPick "categoryFooters"]
	Sanity-Review = true
[access "refs/heads/*"]
	exclusiveGroupPermissions = stage
	submit = group Approvers
