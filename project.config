[project]
	description = The Qlalr repository\n\
\n\
ssh://codereview.qt-project.org:29418/qt/qlalr.git
[receive]
	requireChangeId = true
[submit]
	action = cherry pick
	mergeContent = true
[access "refs/*"]
	owner = group Administrators
	forgeAuthor = group Continuous Integration Bot
	forgeCommitter = group Continuous Integration Bot
	forgeServerAsCommitter = group Continuous Integration Bot
[access "refs/heads/*"]
	create = group Continuous Integration Bot
	push = +force group Continuous Integration Bot
[access "refs/heads/master"]
	submit = group Approvers
